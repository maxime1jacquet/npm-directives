{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "importHelpers": true,
    "esModuleInterop": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "ES2022",
    "moduleResolution": "bundler",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "target": "ES2022",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "ngx-simple-countdown": [
        "dist/ngx-simple-countdown"
      ],
      "ngx-simple-countdown/*": [
        "dist/ngx-simple-countdown/*"
      ],
      "ngx-parallax": [
        "dist/ngx-parallax"
      ],
      "ngx-parallax/*": [
        "dist/ngx-parallax/*"
      ],
      "ngx-cursor": [
        "dist/ngx-cursor"
      ],
      "ngx-cursor/*": [
        "dist/ngx-cursor/*"
      ],
      "ngx-wrapper-tiny-slider": [
        "dist/ngx-wrapper-tiny-slider/ngx-wrapper-tiny-slider",
        "dist/ngx-wrapper-tiny-slider"
      ]
    }
  }
}